<Window x:Class="AutoJTTXUtilities.Controls.Windows.ProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoJTTXUtilities.Controls.Windows"
        mc:Ignorable="d"
        Title="Progress" Height="150" Width="300" MinHeight="150" MinWidth="300"  MaxHeight="150" MaxWidth="300"
        WindowStyle="ToolWindow" WindowStartupLocation="CenterOwner">

    <Grid>
        <ProgressBar Name="ProgressBar" Height="20" Margin="10,0,10,25" Minimum="0" Maximum="100"/>
        <Button Content="Cancel" Width="80" Height="25" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="10,0,10,15" Click="CancelButton_Click"/>
    </Grid>
</Window>
